{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/auth/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/auth/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/auth",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    },
    "notifications": {
      "type": "application",
      "root": "apps/notifications",
      "entryFile": "main",
      "sourceRoot": "apps/notifications/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notifications/tsconfig.app.json"
      }
    },
    "teams": {
      "type": "application",
      "root": "apps/teams",
      "entryFile": "main",
      "sourceRoot": "apps/teams/src",
      "compilerOptions": {
        "tsConfigPath": "apps/teams/tsconfig.app.json"
      }
    },
    "chats": {
      "type": "application",
      "root": "apps/chats",
      "entryFile": "main",
      "sourceRoot": "apps/chats/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chats/tsconfig.app.json"
      }
    },
    "matches": {
      "type": "application",
      "root": "apps/matches",
      "entryFile": "main",
      "sourceRoot": "apps/matches/src",
      "compilerOptions": {
        "tsConfigPath": "apps/matches/tsconfig.app.json"
      }
    }
  }
}